---
layout: 'base.liquid'
permalink: "{{ meta.url }}/index.html"
---

<div class="Blog-title">
    {{ title }}
</div>
<div class="Blog-date">
    {{ meta.date }}
</div>

{% if img != null and img != ""  %}
<div class="Blog-imageFullWidth">
    <img src="{{ img }}"></img>
</div>
{% endif %}

{{ content }}